			function triggeredKey(e) {
				var pattern=/^[0-9-]+$/g;
				if(pattern.test(e)){
					document.getElementById("contact:phone").value = e;
				}
				else{
					var txt = e.slice(0, -1);
					document.getElementById("contact:phone").value = txt;
				}
			}
			function switchLanguage(value) {
				document.cookie = value;
				if (value === "EN") {
					var link = "./en";
					if (document.location.href.includes("#")) {
						var frsublink = document.location.href.split("#")[1];
						var ensublink;
						switch(frsublink) {
							case "attributions":
								ensublink = "awards";
							break;
							case "actualites":
								ensublink = "news";
							break;
							default:
								ensublink = frsublink;
							break;
						}
						if (ensublink != "#") link += "#" + ensublink;
					}
					document.location.href = link;
				}else return;
			}
